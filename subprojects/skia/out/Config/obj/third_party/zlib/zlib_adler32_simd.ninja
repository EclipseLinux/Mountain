defines = -DNDEBUG -DZLIB_IMPLEMENTATION -DADLER32_SIMD_SSSE3 -DX86_NOT_WINDOWS
framework_dirs =
include_dirs =
cflags = -mssse3 -Wno-attributes -fstrict-aliasing -fPIC -fvisibility=hidden -O3 -fdata-sections -ffunction-sections
cflags_c =
root_out_dir = .
target_output_name = zlib_adler32_simd

build obj/third_party/externals/zlib/zlib_adler32_simd.adler32_simd.o: cc ../../third_party/externals/zlib/adler32_simd.c
  source_file_part = adler32_simd.c
  source_name_part = adler32_simd

build obj/third_party/zlib/zlib_adler32_simd.stamp: stamp obj/third_party/externals/zlib/zlib_adler32_simd.adler32_simd.o || obj/third_party/zlib/zlib_common_headers.stamp
